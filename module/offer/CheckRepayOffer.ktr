        <jsScript_script>//Script here
//offerCount  --未回盘笔数
//offerDetail  --未回盘合同编号
var CheckRepayOffer = "";
var color = "";
if (offerCount == 0) {
	CheckRepayOffer = "正常";
	color = "green";
} else {
	CheckRepayOffer = "异常" + offerCount + "笔;明细【" + offerDetail + "】";
	color = "red";
}
CheckRepayOffer = "&lt;font color=\"" + color + "\">" + CheckRepayOffer + "&lt;/font>";</jsScript_script>
